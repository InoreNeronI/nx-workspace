{
  "name": "observable-promise-learning",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/observable-promise-learning/src",
  "prefix": "",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/observable-promise-learning",
        "index": "apps/.dist/observable-promise-learning/src/index.html",
        "main": "apps/observable-promise-learning/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/observable-promise-learning/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [{"glob": "*.*", "input": "apps/.dist", "output": ""}]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "observable-promise-learning:build:production"
        },
        "development": {
          "browserTarget": "observable-promise-learning:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "observable-promise-learning:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": ["zone.js", "zone.js/testing"],
        "tsConfig": "apps/observable-promise-learning/tsconfig.spec.json",
        "inlineStyleLanguage": "scss",
        "assets": [{"glob": "*.*", "input": "apps/.dist", "output": ""}]
      }
    }
  }
}
